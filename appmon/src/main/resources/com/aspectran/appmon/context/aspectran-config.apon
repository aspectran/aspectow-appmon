context: {
    name: appmon
    rules: classpath:com/aspectran/appmon/context/root-context.xml
    scan: [
        com.aspectran.appmon
    ]
    profiles: {
        essential: [
            h2
        ]
        default: [
            dev
        ]
    }
}
scheduler: {
    startDelaySeconds: 2
    waitOnShutdown: true
    enabled: true
}
